class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& matrix) {
         int i,j;
        int m= matrix.size();
        int n= matrix[i].size();
       
        vector<vector<int>>ans(n,vector<int>(m,0));
        for(i=0;i<m;i++)
        {
            for(j=0;j<n;j++)
            {
                ans[j][i]=matrix[i][j];
            }
        }
        return ans;
        }
};
